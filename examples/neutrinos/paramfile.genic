OutputDir = output    # Directory for output
FileBase = IC              # Base-filename of output files

Ngrid = 128

BoxSize = 150000.   # Periodic box size of simulation

Omega0 = 0.2888      # Total matter density  (at z=0)
OmegaLambda = 0.712      # Cosmological constant (at z=0)
OmegaBaryon = 0.00     # Baryon density        (at z=0)
ProduceGas = 0         # 0 is dm only. 1 is gas
HubbleParam = 0.7      # Hubble paramater (may be used for power spec parameterization)

Redshift = 99        # Starting redshift

Sigma8 = -1      # power spectrum normalization
InputPowerRedshift = -1

WhichSpectrum = 2         # "1" selects Eisenstein & Hu spectrum,
# "2" selects a tabulated power spectrum in
# the file 'FileWithInputSpectrum'
# otherwise, Eisenstein & Hu parametrization is used

DifferentTransferFunctions = 1
FileWithTransferFunction = camb_transfer_nu_99.dat
FileWithFutureTransferFunction = camb_transfer_nu_98.99.dat      # filename of tabulated input
InputFutureRedshift = 98.99
FileWithInputSpectrum = camb_matterpow_nu_99.dat      # filename of tabulated input
# spectrum (if used)
InputSpectrum_UnitLength_in_cm = 3.085678e24 # defines length unit of tabulated
# input spectrum in cm/h. 
# Note: This can be chosen different from UnitLength_in_cm

PrimordialIndex = 1. # may be used to tilt the primordial index

Seed = 181170    #  seed for IC-generator

UnitLength_in_cm = 3.085678e21   # defines length unit of output (in cm/h) 
UnitMass_in_g = 1.989e43      # defines mass unit of output (in g/cm)
UnitVelocity_in_cm_per_s = 1e5 # defines velocity unit of output (in cm/sec)

RadiationOn = 1
MNue = 0.133333333333
MNum = 0.133333333333
MNut = 0.133333333333

